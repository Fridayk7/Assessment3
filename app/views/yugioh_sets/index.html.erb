<p id="notice"><%= notice %></p>

<h1>Yugioh Sets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @yugioh_sets.each do |yugioh_set| %>
      <tr>
        <td><%= yugioh_set.name %></td>
        <td><%= yugioh_set.code %></td>
        <td><%= yugioh_set.year %></td>
        <td><%= link_to 'Show', yugioh_set %></td>
        <td><%= link_to 'Edit', edit_yugioh_set_path(yugioh_set) %></td>
        <td><%= link_to 'Destroy', yugioh_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Yugioh Set', new_yugioh_set_path %>
